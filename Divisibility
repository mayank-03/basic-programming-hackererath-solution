#include<iostream>
using namespace std;
int main()
{
    int size;
    cin>>size;
    int num;
    int arr[size];
    for(int i=0;i<size;i++)
    {
        cin>>arr[i];
    }
    for(int i=0;i<size;i++) 
    {
        num=num*10+(arr[i]%10);
    }
    if(num%10==0)
      {
      printf("Yes");
      }
      else
      {
     printf("No");
      }
}
